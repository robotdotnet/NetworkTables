{
  "version": "1.0.0",
  "testRunner": "nunit",
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-1",
    "Microsoft.CodeAnalysis.CSharp": "1.3.2",
    "Microsoft.NETCore.Platforms": "1.0.1",
    "FRC.NetworkTables.Core": {
      "target": "project"
    },
    "NUnit": "3.4.1"
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Threading.Tasks": "4.0.0"
      }
    }, 
    "netcoreapp1.0": {
      "imports": [
        "netcoreapp1.0",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  },
  "buildOptions": {
    "exclude": [
      "Output/",
      "Exceptions/",
      "Tables/",
      "Wire/"
